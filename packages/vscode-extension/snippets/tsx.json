  "Ultra Carousel Custom Animation": {
    "prefix": "uc-custom",
    "body": [
      "import { Carousel, type CustomAnimationFn } from 'react-native-ultra-carousel';",
      "import { interpolate, Extrapolation } from 'react-native-reanimated';",
      "",
      "const ${1:myAnimation}: CustomAnimationFn = (progress, index, total) => {",
      "  'worklet';",
      "",
      "  const ${2:rotate} = interpolate(progress, [-1, 0, 1], [-${3:30}, 0, ${3:30}], Extrapolation.CLAMP);",
      "  const scale = interpolate(Math.abs(progress), [0, 1], [1, ${4:0.8}], Extrapolation.CLAMP);",
      "",
      "  return {",
      "    transform: [",
      "      { rotate: `\\${${2:rotate}}deg` },",
      "      { scale },",
      "    ],",
      "    opacity: interpolate(Math.abs(progress), [0, 1], [1, ${5:0.5}], Extrapolation.CLAMP),",
      "  };",
      "};",
      "",
      "<Carousel",
      "  data={${6:data}}",
      "  renderItem={${7:renderItem}}",
      "  preset={${1:myAnimation}}",
      "  width={${8:350}}",
      "  height={${9:200}}",
      "/>"
    ],
    "description": "Ultra Carousel with custom animation"
  },
  "Ultra Carousel Plugin": {
    "prefix": "uc-plugin",
    "body": [
      "import { createPlugin } from 'react-native-ultra-carousel';",
      "",
      "const ${1:myPlugin} = createPlugin({",
      "  name: '${2:my-plugin}',",
      "  onInit: (carousel) => {",
      "    ${3:// carousel initialized}",
      "  },",
      "  onIndexChange: (index) => {",
      "    ${4:console.log('Index:', index);}",
      "  },",
      "  onDestroy: () => {",
      "    ${5:// cleanup}",
      "  },",
      "});"
    ],
    "description": "Ultra Carousel plugin"
  },
  "Ultra Carousel Preset Import": {
    "prefix": "uc-import",
    "body": [
      "import { Carousel, useCarousel } from 'react-native-ultra-carousel';",
      "import type { PresetName } from 'react-native-ultra-carousel';"
