{
  "Ultra Carousel Basic": {
    "prefix": "uc-basic",
    "body": [
      "import { Carousel } from 'react-native-ultra-carousel';",
      "",
      "<Carousel",
      "  data={${1:data}}",
      "  renderItem={({ item }) => (",
      "    <View style={{ flex: 1 }}>",
      "      <Text>{item.${2:title}}</Text>",
      "    </View>",
      "  )}",
      "  preset=\"${3|slide,fade,slide-fade,scale,scale-fade,vertical,vertical-fade,parallax,overlap,peek|}\"",
      "  width={${4:350}}",
      "  height={${5:200}}",
